{
  "$schema": "http://json-schema.org/draft-07/schema",
  "required": [
    "imageRegistry",
    "imagePullSecret",
    "backendImageTag",
    "ingress"
  ],
  "properties": {
    "imageRegistry": {
      "type": "string"
    },
    "imagePullSecret": {
      "type": "string"
    },
    "backendImageTag": {
      "type": "string"
    },
    "ingress": {
      "type": "object",
      "required": [
        "host"
      ],
      "properties": {
        "host": {
          "type": "string"
        },
        "annotations": {
          "type": "object"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}